<div id="screen-content" data-controller="swipe" >
  <% @profiles.each do |profile| %>
    <div data-swipe-profile-value=<%= profile.id %>>
      <div data-swipe-target="swipeCard" >
        <%= render "profile", profile: profile %>
        <%= render "offer", profile: profile %>
      </div>
    </div>
  <% end %>
</div>
